<?xml version='1.0' encoding='utf-8'?>
<odoo>

    <!-- model.name tree view -->
    <record id="cv_education_view_tree" model="ir.ui.view">
        <field name="name">cv.education Tree</field>
        <field name="model">cv.education</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="title"/>
                <field name="institution"/>
                <field name="start_date"/>
                <field name="end_date"/>
                <field name="description"/>
            </tree>
        </field>
    </record>


</odoo>