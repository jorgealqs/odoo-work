<?xml version='1.0' encoding='utf-8'?>
<odoo>

    <record id="football_fixture_action_action_id" model="ir.actions.act_url">
        <field name="name">Match Games</field>
        <field name="url">/games</field>
        <field name="target">self</field>
    </record>

    <record id="manual_football_action_action_id" model="ir.actions.act_url">
        <field name="name">User Manual</field>
        <field name="url">/manual-football</field>
        <field name="target">self</field>
    </record>

    <menuitem
        id="football_fixture_menu_action"
        name="Match"
        action="football_fixture_action_action_id"
        parent="follow_up_football.football_country_second_level_menu"
        sequence="50"
    />

    <menuitem
        id="manual_football_menu_action"
        action="manual_football_action_action_id"
        parent="follow_up_football.football_country_second_level_menu"
        sequence="60"
    />

</odoo>